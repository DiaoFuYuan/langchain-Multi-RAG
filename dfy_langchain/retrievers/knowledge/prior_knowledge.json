  "query_decomposition_rules": {
    "complex_patterns": [
      {
        "pattern": "关于.*的.*情况",
        "decomposition_strategy": "topic_detail",
        "keywords": ["关于", "情况", "详情", "信息"],
        "sub_queries": [
          "{topic}的基本信息",
          "{topic}的处理情况", 
          "{topic}的相关规定"
        ]
      },
      {
        "pattern": ".*投诉.*处理.*",
        "decomposition_strategy": "process_oriented",
        "keywords": ["投诉", "处理", "流程", "结果"],
        "sub_queries": [
          "投诉内容",
          "处理流程",
          "处理结果",
          "相关责任人"
        ]
      },
      {
        "pattern": ".*事件.*原因.*",
        "decomposition_strategy": "cause_analysis",
        "keywords": ["事件", "原因", "分析", "调查"],
        "sub_queries": [
          "事件经过",
          "事件原因",
          "调查结果",
          "处理措施"
        ]
      },
      {
        "pattern": ".*食品安全.*",
        "decomposition_strategy": "safety_domain",
        "keywords": ["食品安全", "食物中毒", "餐饮", "检查"],
        "sub_queries": [
          "食品安全事件",
          "食品安全检查",
          "食品安全规定",
          "食品安全处理"
        ]
      },
      {
        "pattern": ".*舆情.*处置.*",
        "decomposition_strategy": "crisis_management",
        "keywords": ["舆情", "处置", "应对", "管理"],
        "sub_queries": [
          "舆情事件",
          "处置措施",
          "应对策略",
          "后续跟进"
        ]
      },
      {
        "pattern": ".*市场.*分析.*",
        "decomposition_strategy": "market_analysis",
        "keywords": ["市场", "分析", "趋势", "竞争"],
        "sub_queries": [
          "市场现状",
          "竞争格局",
          "发展趋势",
          "机会威胁"
        ]
      },
      {
        "pattern": ".*女士.*投诉.*内容.*",
        "decomposition_strategy": "person_complaint_content",
        "keywords": ["女士", "投诉", "内容", "反映"],
        "sub_queries": [
          "{person}投诉内容",
          "{person}反映问题",
          "{person}相关记录"
        ]
      },
      {
        "pattern": ".*先生.*投诉.*",
        "decomposition_strategy": "person_complaint_general",
        "keywords": ["先生", "投诉", "举报", "申诉"],
        "sub_queries": [
          "{person}投诉情况",
          "{person}投诉处理",
          "{person}相关信息"
        ]
      },
      {
        "pattern": ".*的投诉内容是什么",
        "decomposition_strategy": "complaint_content_inquiry",
        "keywords": ["投诉", "内容", "什么", "详情"],
        "sub_queries": [
          "{person}投诉详情",
          "{person}反映内容",
          "{person}投诉记录"
        ]
      },
      {
        "pattern": "投诉人.*的.*",
        "decomposition_strategy": "complainant_info",
        "keywords": ["投诉人", "信息", "情况", "记录"],
        "sub_queries": [
          "投诉人信息",
          "投诉内容",
          "处理情况"
        ]
      },
      {
        "pattern": ".*技术.*方案.*",
        "decomposition_strategy": "technical_solution",
        "keywords": ["技术", "方案", "实施", "评估"],
        "sub_queries": [
          "技术需求",
          "解决方案",
          "实施计划",
          "风险评估"
        ]
      },
      {
        "pattern": ".*管理.*流程.*",
        "decomposition_strategy": "management_process",
        "keywords": ["管理", "流程", "制度", "规范"],
        "sub_queries": [
          "管理制度",
          "操作流程",
          "执行规范",
          "监控机制"
        ]
      }
    ],
    "entity_extraction": {
      "person_patterns": [
        ".*先生", ".*女士", ".*同志", ".*经理", ".*总监", ".*专家",
        "投诉人.*", "举报人.*", "申诉人.*", "反映人.*",
        "[王李张刘陈杨黄赵周吴徐孙朱马胡郭林何高梁郑罗宋谢唐韩曹许邓萧冯曾程蔡彭潘袁于董余苏叶吕魏蒋田杜丁沈姜范江傅钟卢汪戴崔任陆廖姚方金邱夏谭韦贾邹石熊孟秦阎薛侯雷白龙段郝孔邵史毛常万顾赖武康贺严尹钱施牛洪龚汤陶黎温莫易樊乔文安殷颜庄章鲁倪庞邢俞翟蓝聂丛岳齐沿][一-龥]{1,3}",
        "[一-龥]{2,4}(先生|女士|同志|老师|医生|护士|主任|经理|总监|专家|局长|处长|科长|主管|员工|客户|患者|学生|家长)"
      ],
      "organization_patterns": [".*局", ".*办", ".*中心", ".*学校", ".*医院", ".*公司", ".*集团", ".*部门"],
      "location_patterns": [".*市", ".*区", ".*县", ".*街道", ".*路", ".*省", ".*国家", ".*地区"],
      "time_patterns": ["\\d{4}年", "\\d{1,2}月", "\\d{1,2}日", "\\d{4}-\\d{2}-\\d{2}", "\\d{4}年\\d{1,2}月"],
      "number_patterns": ["\\d+人", "\\d+例", "\\d+起", "\\d+件", "\\d+万", "\\d+亿", "\\d+%"],
      "product_patterns": [".*产品", ".*系统", ".*平台", ".*服务", ".*解决方案"],
      "technology_patterns": [".*技术", ".*算法", ".*框架", ".*架构", ".*标准"],
      "complaint_patterns": [
        "投诉.*", "举报.*", "反映.*", "申诉.*", "意见.*", "建议.*",
        ".*投诉内容", ".*投诉情况", ".*投诉处理", ".*投诉记录"
      ]
    }
  },
